{
  "name": "inventory-api",
  "version": "1.0.0",
  "description": "Inventory Api Ooh challenge",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "dev": "npm install && tsc-watch --noClear --onSuccess \"node ./build/src/index.js\"",
    "build": "tsc",
    "start": "node ./build/index.js",
    "migrate": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "getJwt": "./node_modules/.bin/ts-node ./scripts/makeJwt.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "Varun Naik <varun@vnaik.com>",
  "license": "ISC",
  "dependencies": {
    "celebrate": "10.1.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jest": "24.9.0",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.9.1",
    "pg": "7.14.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.25",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/compression": "1.0.1",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/hapi__joi": "15.0.4",
    "@types/jest": "24.0.23",
    "@types/jsonwebtoken": "8.3.5",
    "@types/morgan": "1.7.37",
    "@types/supertest": "2.0.8",
    "@types/uuid": "3.4.6",
    "@types/winston": "2.4.4",
    "husky": "3.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.2",
    "tsc-watch": "4.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.2",
    "uuid": "3.3.3"
  }
}
